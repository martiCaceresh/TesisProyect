@model OptiArroz_Tesis_Proyect.Models.Utils.AlertModel





<!-- Lógica de SweetAlert2 -->
<script>

    $(document).ready(function () {
        Swal.fire({
            icon: '@Model.Type', // El tipo de alerta (success, error, info, etc.)
            title: '@Model.Title', // El mensaje de la alerta
            text: '@Model.Message',
            showConfirmButton: true // Muestra un botón de confirmación
        });
    });

   
</script>
